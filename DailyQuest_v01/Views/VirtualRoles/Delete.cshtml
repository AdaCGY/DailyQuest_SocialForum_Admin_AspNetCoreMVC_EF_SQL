@model DailyQuest_v01.Models.VirtualRole

@{
    ViewData["Title"] = "Delete";
}

<h1>刪除虛擬角色</h1>

@* <h3>您確定要刪除?</h3> *@
<div>
    @* <h4>VirtualRole</h4> *@
    <hr />
    <dl class="row">
        <dt class="col-sm-2 mb-4">
            角色編號
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RoleId)
        </dd>
        <dt class="col-sm-2 mb-4">
            角色名稱
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RoleName)
        </dd>
        <dt class="col-sm-2 mb-4">
            角色描述
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RoleDescription)
        </dd>
        <dt class="col-sm-2 mb-4">
            角色照片
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RolePhoto)
        </dd>
        <dt class="col-sm-2 mb-4">
            創立時間
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>
        <dt class="col-sm-2 mb-4">
            修改時間
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastModified)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="RoleId" />
        <input type="submit" value="刪除" class="btn btn-danger" /> 
        @* <a asp-action="Index">返回</a> *@
    </form>
</div>
<br />
<div>
    <a asp-action="Index">返回</a>
</div>
